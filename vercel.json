{
    "version": 2,
    "builds": [
      {
        "src": "nuxt.config.js",
        "use": "@nuxtjs/vercel-builder"
      },
      {
        "src": "api/*.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/health",
        "dest": "api/health.py"
      },
      {
        "src": "/api/query",
        "dest": "api/query.py"
      }
    ],
    "env": {
        "REDIS_URL": "@redis_url",
        "OPENAI_API_KEY": "@openai_api_key",
        "OPENAI_API_BASE": "@openai_api_base",
        "OPENAI_MODEL_NAME": "@openai_model_name"
    }
  }
  